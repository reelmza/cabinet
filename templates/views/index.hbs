          <input type="text" name="username" id="login-username" placeholder="e.g: jamal2020" required
            autocomplete="username" />

          <label for="password">PASSWORD</label>
          <input type="password" name="password" id="login-password" placeholder="****" required autocomplete="" />

          <div class="myAlert" style="display:none; ">
            Username or Password Incorect
