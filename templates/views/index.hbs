          <button type="submit">LOGIN <i class="fas fa-paper-plane"></i></button>
          <div id="login-alternatives">
            <a href="">Cancel</a> <br>
            <a href="" id="login-register-link">Don't have account? Register</a>
          </div>
        </form>
      </div>

