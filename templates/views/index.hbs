      <div id="register-form-box">
        <form id="register-form">
          <div class="row">
            <div class="col-12 col-sm-4">
              <label for="register-name">SCHOOL NAME</label>
              <input type="text" name="register-name" id="register-name" placeholder="EG: Saint James" />
            </div>
            <div class="col-12 col-sm-4">
              <label for="register-type">SCHOOL TYPE</label>
              <select name="register-type" id="register-type" required>
                <option value=""></option>
                <option value="High School"> High School</option>
                <option value="Primary School"> Primary School</option>
                <option value="Vocational School"> Vocational School</option>
              </select>
            </div>
            <div class="col-12 col-sm-4">
              <label for="register-student-number">SCHOOL POPULATION</label>
